{
  "name": "t-wedding",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node src/app",
    "build": "tsc",
    "build:proto": "ts-proto --protoFiles=./src/proto --outputDir=./dist/proto",
    "test": "cross-env NODE_ENV=development jest --detectOpenHandles",
    "start:dev": "cross-env NODE_ENV=development nodemon src/app.ts",
    "start:prod": "cross-env NODE_ENV=production nodemon dist/app",
    "start:debug": "cross-env NODE_ENV=development nodemon inspect src/app.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js",
    "format:write": "npx prettier --write \"src/**/*.js\" --ignore-path ./.prettierignore",
    "format:check": "npx prettier --check \"src/**/*.js\" --ignore-path ./.prettierignore",
    "jest": "jest --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/proto-loader": "^0.7.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^6.7.0",
    "graphql": "^15.8.0",
    "grpc": "^1.24.11",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.2",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.151.0",
    "typescript": "^5.1.6"
  }
}
